{
  "name": "webit-de/debug-settings-task",
  "description": "Routinely reset TYPO3 debug settings to the »live« preset on production",
  "homepage": "https://github.com/webit-de/typo3-debug_settings_task",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "require": {
    "php": ">=7.2.0",
    "typo3/cms-core": "^9.5 || ^10.4"
  },
  "autoload": {
    "psr-4": {"WebitDe\\DebugSettingsTask\\": "Classes/"}
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "debug_settings_task"
    }
  },
  "replace": {
    "typo3-ter/debug-settings-task": "self.version"
  }
}
